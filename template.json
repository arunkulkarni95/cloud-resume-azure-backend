{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "sites_arun_cloud_resume_name": {
            "defaultValue": "arun-cloud-resume",
            "type": "String"
        },
        "databaseAccounts_arun_cloud_resume_name": {
            "defaultValue": "arun-cloud-resume",
            "type": "String"
        },
        "serverfarms_ASP_cloudresumechallenge_bb87_externalid": {
            "defaultValue": "/subscriptions/28ff1ba9-8c59-461a-a8f0-d5e22c6145fa/resourceGroups/cloud-resume-challenge/providers/Microsoft.Web/serverfarms/ASP-cloudresumechallenge-bb87",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.DocumentDB/databaseAccounts",
            "apiVersion": "2023-09-15",
            "name": "[parameters('databaseAccounts_arun_cloud_resume_name')]",
            "location": "West US",
            "tags": {
                "defaultExperience": "Azure Table",
                "hidden-cosmos-mmspecial": ""
            },
            "kind": "GlobalDocumentDB",
            "identity": {
                "type": "None"
            },
            "properties": {
                "publicNetworkAccess": "Enabled",
                "enableAutomaticFailover": false,
                "enableMultipleWriteLocations": false,
                "isVirtualNetworkFilterEnabled": false,
                "virtualNetworkRules": [],
                "disableKeyBasedMetadataWriteAccess": false,
                "enableFreeTier": false,
                "enableAnalyticalStorage": false,
                "analyticalStorageConfiguration": {
                    "schemaType": "WellDefined"
                },
                "databaseAccountOfferType": "Standard",
                "defaultIdentity": "FirstPartyIdentity",
                "networkAclBypass": "None",
                "disableLocalAuth": false,
                "enablePartitionMerge": false,
                "enableBurstCapacity": false,
                "minimalTlsVersion": "Tls12",
                "consistencyPolicy": {
                    "defaultConsistencyLevel": "BoundedStaleness",
                    "maxIntervalInSeconds": 86400,
                    "maxStalenessPrefix": 1000000
                },
                "locations": [
                    {
                        "locationName": "West US",
                        "provisioningState": "Succeeded",
                        "failoverPriority": 0,
                        "isZoneRedundant": false
                    }
                ],
                "cors": [],
                "capabilities": [
                    {
                        "name": "EnableTable"
                    },
                    {
                        "name": "EnableServerless"
                    }
                ],
                "ipRules": [],
                "backupPolicy": {
                    "type": "Periodic",
                    "periodicModeProperties": {
                        "backupIntervalInMinutes": 240,
                        "backupRetentionIntervalInHours": 8,
                        "backupStorageRedundancy": "Geo"
                    }
                },
                "networkAclBypassResourceIds": [],
                "capacity": {
                    "totalThroughputLimit": 4000
                },
                "keysMetadata": {}
            }
        },
        {
            "type": "Microsoft.DocumentDB/databaseAccounts/tables",
            "apiVersion": "2023-09-15",
            "name": "[concat(parameters('databaseAccounts_arun_cloud_resume_name'), '/VisitorCount')]",
            "dependsOn": [
                "[resourceId('Microsoft.DocumentDB/databaseAccounts', parameters('databaseAccounts_arun_cloud_resume_name'))]"
            ],
            "properties": {
                "resource": {
                    "id": "VisitorCount"
                }
            }
        },
        {
            "type": "Microsoft.Web/sites/functions",
            "apiVersion": "2023-01-01",
            "name": "[concat(parameters('sites_arun_cloud_resume_name'), '/update_visitor_count')]",
            "location": "East US",
            "dependsOn": [
                "[resourceId('Microsoft.Web/sites', parameters('sites_arun_cloud_resume_name'))]"
            ],
            "properties": {
                "script_href": "https://arun-cloud-resume.azurewebsites.net/admin/vfs/home/site/wwwroot/function_app.py",
                "test_data_href": "https://arun-cloud-resume.azurewebsites.net/admin/vfs/tmp/FunctionsData/update_visitor_count.dat",
                "href": "https://arun-cloud-resume.azurewebsites.net/admin/functions/update_visitor_count",
                "config": {},
                "invoke_url_template": "https://arun-cloud-resume.azurewebsites.net/api/update_visitor_count",
                "language": "python",
                "isDisabled": false
            }
        }
    ]
}